{
    "name": "SpartaPark",
    "description": "SpartaPark - A Parking Guidance System.",
    "keywords": ["SpartaPark", "SJSU", "parking", "garage"],
    "require": {
        "laravel/framework": "4.2.*",
        "way/generators": "3.0.*",
        "barryvdh/laravel-ide-helper": "1.11.*",
        "fzaninotto/faker": "1.4.*",
        "appitventures/phpgmaps": "dev-master",
        "torann/geoip": "dev-master",
        "knyga/getip": "dev-master",
        "toin0u/geocoder-laravel": "@stable",
        "kriswallsmith/buzz": "dev-master",
        "zendframework/zend-http": "2.3.*@dev",
        "geoip/geoip": "dev-master",
        "intervention/image": "2.*",
        "intervention/imagecache": "2.*",
        "doctrine/dbal": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.9.*",
        "league/factory-muffin": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "SpartaPark\\": "app/SpartaPark/"
        },
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/models",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan ide-helper:generate",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"
}